<%= simple_form_for @game, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.association :word %>
  <%= error_span(@game[:word]) %>

  <%= f.button :submit, :class => 'btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                lobby_index_path, :class => 'btn btn-default' %>
<% end %>
